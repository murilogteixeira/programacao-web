package com.murilo.web;

public class Calculadora {
	
	Double resultadoDouble;
	Integer resultadoInteger;
	
	static Integer calcular(Integer num1, Integer num2, String operador) {
		switch (operador) {
		case "+":
			return(num1 + num2);

		case "-":
			return(num1 - num2);

		case "*":
			return(num1 * num2);

		case "/":
			return(num1 / num2);

		default:
			return(null);
		}
	}
	
	static Double calcular(Double num1, Double num2, String operador) {
		switch (operador) {
		case "+":
			return(num1 + num2);

		case "-":
			return(num1 - num2);

		case "*":
			return(num1 * num2);

		case "/":
			return(num1 / num2);

		default:
			return(null);
		}
	}
	
	@Override
	public String toString() {
		return "";
	}
}
